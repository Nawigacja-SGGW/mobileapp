{
  "expo": {
    "name": "nawigacjaSGGW-mobile",
    "slug": "nawigacjaSGGW-mobile",
    "version": "1.0.0",
    "scheme": "nawigacjaSGGW-mobile",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-localization",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      "@maplibre/maplibre-react-native"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/nawigacja-SGGW.png",
    "splash": {
      "image": "./assets/nawigacja-SGGW.png",
      "resizeMode": "contain",
      "backgroundColor": "#274839"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/nawigacja-SGGW.png",
        "backgroundColor": "#274839"
      },
      "package": "com.anonymous.nawigacjaSGGWmobile"
    },
    "updates": {
      "enabled": true
    }
  }
}
